<%= simple_form_for @cliente do |f| %>

  <%= f.input :documento %>  
  <%= f.input :numero %>
  <%= f.input :nombre %>
  <%= f.input :direcion %>
  <%= f.input :lugar %>  
  <%= f.input :telefono %>
  <%= f.input :movil %>  
  <%= f.input :correo %>  
  <%= f.input :empleado %>

  <div class="actions">
    <%= f.submit "Guardar", class: "btn btn-success" %>
  </div>
<% end %>
