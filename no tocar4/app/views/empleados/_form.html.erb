<%= simple_form_for @empleado do |f| %>

    <%= f.association :documento %>
  
    <%= f.input :numero %>
  
    <%= f.input :nombre %>
  
    <%= f.input :direcion %>
  
    <%= f.input :lugar %>
  
    <%= f.input :telefono %>
  
    <%= f.input :movil %>
  
    <%= f.input :correo %>
  
    <%= f.input :usuario %>
  
    <%= f.input :contrasena %>
  

  <div class="actions">
    <%= f.submit "guardar", class:"btn btn-success" %>
  </div>
<% end %>
